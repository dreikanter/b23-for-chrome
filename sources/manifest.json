{
	"name": "B23",
	"version": "1.0.32",
	"description": "Client plugin for b23.ru, URL shortening service.",
	"author": "Alex Musayev",  
	"icons":
	{
		"16": "images/b23-16.png",
		"48": "images/b23-48.png",
		"128": "images/b23-128.png"
	},
	"options_page":	"settings.html",
	"browser_action":
	{
		"default_icon": "images/b23-16.png",
		"default_title": "23!",
		"popup": "popup.html"
	},
	"permissions": [ 
	"tabs","http://*/","https://*/"
	]
}